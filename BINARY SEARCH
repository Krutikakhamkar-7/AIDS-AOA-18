#include<stdio.h>
#include<conio.h>

int binary_search(int A[],int n,int key){
  int low=0,high=n-1, mid;

  while(low<=high){
  mid=(low+high)/2;

  if (A[mid]==key)
   return mid;
  else if (A[mid]<key)
   low=mid+1;
  else
   high=mid-1;
  }
 return -1;
}

 void main() {
  int A[10],n,key,i,result;
 clrscr();

 printf("enter no of elements:");
 scanf("%d",&n);

 printf("enter sorted array element:\n",n);
 for(i=0;i<n;i++){
 printf("elements %d:",i+1);
 scanf("%d",&A[i]);

  printf("enter the key:");
  scanf("%d",&key);

  result=binary_search(A,n,key);

 if(result!=-1)
 printf("\n key found at position %d(index%d)",result+1,result);
   else
    printf("\n key not found!");
    getch();

}
}
